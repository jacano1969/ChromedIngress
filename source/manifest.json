{
  "name": "Chromed Ingress",
  "default_locale": "uk",
  "version": "0.3.6",
  "manifest_version": 2,
  "browser_action": 
  {
    "default_title": "Chromed Ingress",
    "default_icon": "img/ingress_logo.png"
  },
  "background": 
  {
    "scripts": ["background.js"]
  },
  "permissions": 
  [
    "storage",
    "tabs",
    "notifications",
    "http://ingress.com/intel",
    "http://www.ingress.com/intel"
  ],
  "web_accessible_resources":
  [
    "res/notice.mp3"
  ],
  "options_page": "options.html"
}